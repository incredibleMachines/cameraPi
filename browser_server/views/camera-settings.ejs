<% include includes/header %>
<select class="camera">
  <option value="all">All Cameras</option>
  <% cameras.forEach(function(camera){ %>
      <option value="<%= camera.address %>"><%= camera.camera_id  %></option>
  <% }) %>
</select>
<form method="get" class="imageformat">
